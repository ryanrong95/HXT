                                    <li v-for="(item,index) in navdata" v-on:click.stop="navlist(index,false)"><a v-bind:class="{active:indexData.navIndex==index}">{{item}}</a><span v-bind:class="{spanactive:indexData.navIndex==index}"></span></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div v-on:click.stop="updateds" class="filtrates">
                    <a>筛选</a>
                    <i class="el-icon-arrow-down unfold_icon"></i>
                    <span></span>
                </div>
            </div>

            <div v-if='indexData.show_condition' class="show_condition">
                <div class="condition_box">
                    <div class="condition_list">
                        <p class="condition_title">订单日期</p>
                        <div class="item_condition">
                            <span v-for="(item,index) in order_time" v-on:click.stop="setclass(index,item)"
                                  :class="{condition_active:indexData.categoryIndex==index}">{{item.text}}</span>
                        </div>
                    </div>
                    <div class="condition_list">
                        <p class="condition_title">订单状态</p>
                        <div class="item_condition">
                            <span v-for="(item,index) in order_status" v-on:click.stop="setstatusindex(index)"
                                  :class="{condition_active:indexData.statusindex==index}">{{item}}</span>
                        </div>
                    </div>
                    <div class="condition_list">
                        <p class="condition_title">开票状态</p>
                        <div class="item_condition">
                            <span v-for="(item,index) in invoice_list" v-on:click.stop="setinoviceindex(index,item)"
                                  :class="{condition_active:indexData.inoviceindex==index}">{{item.text}}</span>
                        </div>
                    </div>
                    <div class="condition_list">
                        <p class="condition_title">付汇状态</p>
                        <div class="item_condition">
                            <span v-for="(item,index) in payment_list" v-on:click.stop="setpaymentindex(index,item)"
                                  :class="{condition_active:indexData.paymentindex==index}">{{item.text}}</span>
                        </div>
                    </div>
                </div>
                <div class="condition_btns">
                    <span v-on:click.stop="set_empty">清空条件</span>
                    <span v-on:click.stop="confirm_btn">确定</span>
                </div>
            </div>
            <div id="slider1" class="swipe">
                <ul class="box01_list">
                    <!-- 全部 -->
                    <li class="li_list">
                        <div v-if="!indexData.isNullOrder" ref="viewBox1" v-on:scroll="ss('viewBox1')" class="set_scroll" v-infinite-scroll="loadMore"
                             infinite-scroll-disabled="indexData.infiniteScroll"
                             infinite-scroll-distance="20"
                             infinite-scroll-immediate-check="false">
                            <div class="listpublic all_order_box" v-on:click="orderDetails(item.ID)" v-for="item in indexData.getdata">
                                <div class="all_order">
                                    <div class="all_order_titles">
                                        <img src="~/Content/img/times.png" alt="">
                                        <p>
                                            <span>{{item.CreateDate}}</span>
                                            <span>{{item.OrderStatus}}</span>
                                        </p>
                                    </div>
                                    <div class="all_order_content">
                                        <div class="order_number">
                                            <p>订单编号</p>
                                            <p>{{item.ID}}</p>
                                        </div>
                                        <div class='order_detail'>
                                            <p class="max_height">{{item.PaySuppliersName}}</p>
                                            <p class="order_detail_payment">付汇状态：{{item.PayExchangeStatus}}</p>
                                            <p class="order_detail_payment">可付汇：{{item.Remittance}}</p>
                                            <p class="order_detail_payment">
                                                已付汇：{{item.Remittanced}} <span>{{item.InvoiceStatus}}</span>
                                            </p>
                                        </div>
                                    </div>
                                    <div class="all_order_price">
                                        <p><span>交易币种：{{item.Currency}}</span></p>
                                        <p><span>报关总价：<span class="price">{{item.DeclarePrice}}</span></span></p>
                                        <div class="all_order_invoice">
                                            <span v-on:click.stop="orderInvoice(item.ID)">查看发票</span>
                                            <span class="invocelist" v-if="item.isShowLog" v-on:click.stop="newLog(item.ID)">订单跟踪</span>
                                            <span class="invocelist" v-if="item.isShowBill" v-on:click.stop="orderBills(item.ID)">对账单</span>
                                            <span class="invocelist details_btn" v-on:click.stop="orderDetails(item.ID)">订单详情</span>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="order_kindmore">
                                <mt-spinner v-if="indexData.allTotal>indexData.getdata.length&&indexData.infiniteScroll==false" type="fading-circle"></mt-spinner>
                                <p v-if="indexData.allTotal==indexData.getdata.length&&indexData.infiniteScroll==true&&indexData.allTotal>10">全部加载完成</p>
                            </div>
